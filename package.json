{
  "name": "legacyhaveyousin",
  "version": "1.0.0",
  "description": "LEGACY - haveYouSin is a web app on which you can create a to watch list with tv shows and movies using the movie database API v4(https://developers.themoviedb.org/4/getting-started). This app is built with angulars js (1.6), webpack 3.6, ui-router and materialize css framework. You can visit this legacy version here: https://purdeychambraud.com/legacy-haveyousin",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devserver": "./node_modules/.bin/webpack-dev-server --devtool eval-cheap-module-source-map",
    "clean": "rm -rf dist",
    "build": "export NODE_ENV=production; npm run clean && webpack --define process.env.NODE_ENV='\"production\"'",
    "serve": "export NODE_ENV=development; webpack-dev-server --define process.env.NODE_ENV='\"development\"' --open --watch ",
    "serve-prod": "export NODE_ENV=production; webpack-dev-server --define process.env.NODE_ENV='\"production\"' --open --watch "
  },
  "author": "Purdey Chambraud",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@uirouter/angularjs": "^1.0.8",
    "angular": "^1.7.9",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "font-awesome": "^4.7.0",
    "materialize-css": "^0.100.2"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.5"
  }
}
